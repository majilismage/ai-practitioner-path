            padding: 0.75rem 0;
            color: var(--text-secondary);
            position: relative;
            padding-left: 2rem;
            border-bottom: 1px solid var(--border);
        }

        .prerequisites-list li:last-child {
            border-bottom: none;
        }

        .prerequisites-list li::before {
            content: "‚úì";
            position: absolute;
            left: 0;
            color: var(--success);
            font-weight: bold;
            font-size: 1.1rem;
        }

        /* Checkbox styling */
        .checkbox {
            margin: 2rem 0;
            display: flex;
            align-items: center;
