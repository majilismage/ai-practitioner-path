                body.removeAttribute('data-theme');
                themeText.innerHTML = 'üåô Dark Mode';
                localStorage.setItem('theme', 'dark');
            } else {
                body.setAttribute('data-theme', 'light');
                themeText.innerHTML = '‚òÄÔ∏è Light Mode';
                localStorage.setItem('theme', 'light');
            }
        }

        // Load saved theme
        function loadTheme() {
            const savedTheme = localStorage.getItem('theme');
            if (savedTheme === 'light') {
                document.body.setAttribute('data-theme', 'light');
                document.getElementById('theme-text').innerHTML = '‚òÄÔ∏è Light Mode';
            }
        }

        // Progress tracking
        function markModuleComplete(mission, module) {
            if (!progress[mission]) progress[mission] = {};
            progress[mission][module] = Date.now();
            saveProgress();
            updateProgressDisplay();
            updateModuleStatus();
            updateJourneyMap();
        }

        function isModuleComplete(mission, module) {
            return progress[mission] && progress[mission][module];
        }

        function isMissionComplete(mission) {
            const moduleCount = mission === 4 ? 4 : 3;
            for (let i = 1; i <= moduleCount; i++) {
                if (!isModuleComplete(mission, i)) return false;
            }
            return true;
        }

        function saveProgress() {
            localStorage.setItem('ai-practitioner-progress', JSON.stringify(progress));
        }

        function updateProgressDisplay() {
            const totalModules = 18; // 3+3+3+4+3
            let completedModules = 0;
            
            for (let mission = 1; mission <= 5; mission++) {
                const moduleCount = mission === 4 ? 4 : 3;
                for (let module = 1; module <= moduleCount; module++) {
                    if (isModuleComplete(mission, module)) completedModules++;
                }
            }

            const percentage = Math.round((completedModules / totalModules) * 100);
            document.getElementById('progress-text').textContent = `${percentage}% Complete`;
            document.getElementById('progress-fill').style.width = `${percentage}%`;
        }

        function updateModuleStatus() {
            for (let mission = 1; mission <= 5; mission++) {
                const missionStatus = document.getElementById(`mission-${mission}-status`);
                if (isMissionComplete(mission)) {
                    missionStatus.className = 'mission-status complete';
                    missionStatus.innerHTML = '‚úì';
                } else {
                    missionStatus.className = 'mission-status incomplete';
                    missionStatus.textContent = mission;
                }

                const moduleCount = mission === 4 ? 4 : 3;
                for (let module = 1; module <= moduleCount; module++) {
                    const moduleStatus = document.getElementById(`module-${mission}-${module}-status`);
                    if (moduleStatus) {
                        moduleStatus.className = isModuleComplete(mission, module) ? 
                            'module-status complete' : 'module-status';
                    }
                }
            }
        }

        function updateJourneyMap() {
            for (let i = 1; i <= 5; i++) {
                const journeyIcon = document.getElementById(`journey-${i}`);
                if (isMissionComplete(i)) {
                    journeyIcon.className = 'journey-step-icon complete';
                    journeyIcon.innerHTML = '‚úì';
                } else {
                    // Find first incomplete mission
                    let firstIncomplete = null;
                    for (let j = 1; j <= 5; j++) {
                        if (!isMissionComplete(j)) {
                            firstIncomplete = j;
                            break;
                        }
                    }
                    
                    if (i === firstIncomplete) {
                        journeyIcon.className = 'journey-step-icon current';
                        journeyIcon.textContent = i;
                    } else {
                        journeyIcon.className = 'journey-step-icon';
                        journeyIcon.textContent = i;
                    }
                }
            }
        }

        // Navigation
        function showPage(page) {
            // Hide all pages
            document.querySelectorAll('.page').forEach(p => p.classList.add('hidden'));
            
            // Show target page
            document.getElementById(`page-${page}`).classList.remove('hidden');
            
            // Update nav
            document.querySelectorAll('.nav-link').forEach(link => link.classList.remove('active'));
            document.querySelector(`[onclick="showPage('${page}')"]`).classList.add('active');
            
            currentPage = page;
        }

        function showModule(mission, module) {
            const moduleData = modules[mission] && modules[mission][module];
            if (!moduleData) {
                // For modules not yet implemented, show a placeholder
                const placeholderContent = `
                    <div class="content-header">
                        <div class="badge">Module ${mission}.${module}</div>
                        <h1 class="page-title">Module Coming Soon</h1>
                        <p class="page-subtitle">This module is currently being developed. Please check back later.</p>
                    </div>
                    <div class="content-body">
                        <div class="card">
                            <h3 class="card-title">Module in Development</h3>
                            <p style="color: var(--text-secondary);">
                                This module will be available soon. In the meantime, you can explore the other available modules 
                                or check out the resource library for additional learning materials.
                            </p>
                        </div>
                        <div style="display: flex; gap: 16px; margin-top: 32px;">
                            <button class="btn btn-secondary" onclick="navigateModule(${mission}, ${module}, -1)">‚Üê Previous</button>
                            <button class="btn btn-primary" onclick="navigateModule(${mission}, ${module}, 1)">Next ‚Üí</button>
                        </div>
                    </div>
                `;
                document.getElementById('module-content').innerHTML = placeholderContent;
                document.getElementById('module-content').classList.remove('hidden');
                
                // Update nav
                document.querySelectorAll('.nav-link, .module-link').forEach(link => link.classList.remove('active'));
                const moduleLink = document.querySelector(`[onclick="showModule(${mission}, ${module})"]`);
                if (moduleLink) moduleLink.classList.add('active');
                
                // Expand mission if needed
                const missionModules = document.getElementById(`mission-${mission}-modules`);
                if (!missionModules.classList.contains('expanded')) {
                    toggleMission(mission);
                }

                currentPage = `module-${mission}-${module}`;
                return;
            }

            // Hide all pages
            document.querySelectorAll('.page').forEach(p => p.classList.add('hidden'));
            
            // Generate recall challenge if needed
            let recallChallenge = '';
            if (mission > 1) {
                recallChallenge = generateRecallChallenge(mission);
            }

            // Generate module content
            const moduleContent = `
                <div class="content-header">
                    <div class="badge">${moduleData.subtitle}</div>
                    <h1 class="page-title">${moduleData.title}</h1>
                    <p class="page-subtitle">${moduleData.briefing}</p>
                </div>
                <div class="content-body">
                    ${recallChallenge}
                    
                    <div class="section">
                        <h2 class="section-title">üìã Briefing</h2>
                        <div class="section-content">
                            ${moduleData.briefingContent.split('\n\n').map(p => `<p>${p}</p>`).join('')}
                        </div>
                    </div>

                    <div class="section">
                        <h2 class="section-title">üéØ Core</h2>
                        <div class="section-content">
                            ${formatContent(moduleData.coreContent)}
                        </div>
                    </div>

                    <div class="section">
                        <h2 class="section-title">üõ†Ô∏è Exercise</h2>
                        <div class="section-content">
                            ${formatContent(moduleData.exerciseContent)}
                        </div>
                    </div>

                    <div class="section">
                        <h2 class="section-title">ü§î Debrief</h2>
                        <div class="section-content">
                            ${formatContent(moduleData.debriefContent)}
                        </div>
                    </div>

                    <div class="checkbox">
                        <input type="checkbox" id="complete-${mission}-${module}" 
                               ${isModuleComplete(mission, module) ? 'checked' : ''}
                               onchange="handleModuleComplete(${mission}, ${module}, this.checked)">
                        <label for="complete-${mission}-${module}">Mark this module as complete</label>
                    </div>

                    <div style="display: flex; gap: 16px; margin-top: 32px;">
                        <button class="btn btn-secondary" onclick="navigateModule(${mission}, ${module}, -1)">‚Üê Previous</button>
                        <button class="btn btn-primary" onclick="navigateModule(${mission}, ${module}, 1)">Next ‚Üí</button>
                    </div>
                </div>
            `;

            document.getElementById('module-content').innerHTML = moduleContent;
            document.getElementById('module-content').classList.remove('hidden');
            
            // Update nav
            document.querySelectorAll('.nav-link, .module-link').forEach(link => link.classList.remove('active'));
            const moduleLink = document.querySelector(`[onclick="showModule(${mission}, ${module})"]`);
            if (moduleLink) moduleLink.classList.add('active');
            
            // Expand mission if needed
            const missionModules = document.getElementById(`mission-${mission}-modules`);
            if (!missionModules.classList.contains('expanded')) {
                toggleMission(mission);
            }

            currentPage = `module-${mission}-${module}`;
        }

        function formatContent(content) {
            return content
                .replace(/\*\*([^*]+)\*\*/g, '<strong>$1</strong>')
                .replace(/\*([^*]+)\*/g, '<em>$1</em>')
                .split('\n\n')
                .map(paragraph => {
                    if (paragraph.trim().startsWith('- ') || paragraph.trim().startsWith('* ')) {
                        const items = paragraph.split('\n').filter(line => line.trim());
                        return '<ul>' + items.map(item => `<li>${item.replace(/^[\-\*]\s*/, '')}</li>`).join('') + '</ul>';
                    }
                    return `<p>${paragraph}</p>`;
                })
                .join('');
        }

        function generateRecallChallenge(mission) {
            const challenges = {
                2: [
                    "Explain the CRAFT framework in one sentence without looking it up.",
                    "Name two specific things Microsoft Copilot does exceptionally well.",
                    "What's the difference between using AI as a tool vs. building an AI system?"
                ],
                3: [
                    "Which AI model would you choose for creative brainstorming and why?",
                    "What is a context window and why does it matter?",
                    "Describe one key insight from your vibe coding exercise."
                ],
                4: [
                    "What's the difference between Claude Code and regular Claude chat?",
                    "Name the three MCP primitives and give an example of each.",
                    "When would you use Cowork instead of regular Claude?"
                ],
                5: [
                    "What's the difference between a Skill and an Agent? Explain in one sentence each.",
                    "Name three things OpenClaw can do that a Claude chat subscription cannot.",
                    "Describe Jeffrey Emanuel's agent stress test technique in your own words.",
                    "What are guardrails in the context of AI agents, and why do they matter?"
                ]
            };

            const questions = challenges[mission] || [];
            if (questions.length === 0) return '';

            return `
                <div class="recall-challenge">
                    <button class="recall-toggle" onclick="toggleRecall(this)">
                        üß† Recall Challenge: Mission ${mission-1} Review
                        <span>‚ñº</span>
                    </button>
                    <div class="recall-content">
                        <div>
                            <p style="color: var(--text-secondary); margin-bottom: 16px;">
                                Before diving into new material, test your retention from the previous mission:
                            </p>
                            <ol class="recall-questions">
                                ${questions.map(q => `<li>${q}</li>`).join('')}
                            </ol>
                            <p style="color: var(--text-muted); font-size: 14px; margin-top: 16px;">
                                Don't look back at the previous modules. Write down your answers, then check them later.
                            </p>
                        </div>
                    </div>
                </div>
            `;
        }

        function toggleRecall(button) {
            const content = button.nextElementSibling;
            const arrow = button.querySelector('span');
            
            if (content.classList.contains('expanded')) {
                content.classList.remove('expanded');
                arrow.textContent = '‚ñº';
            } else {
                content.classList.add('expanded');
                arrow.textContent = '‚ñ≤';
            }
        }

        function handleModuleComplete(mission, module, completed) {
            if (completed) {
                markModuleComplete(mission, module);
            } else {
                // Remove completion
                if (progress[mission]) {
                    delete progress[mission][module];
                }
                saveProgress();
                updateProgressDisplay();
                updateModuleStatus();
                updateJourneyMap();
            }
        }

        function navigateModule(currentMission, currentModule, direction) {
            // Calculate next module
            let nextMission = currentMission;
            let nextModule = currentModule + direction;

            // Handle module boundaries
            const moduleCount = currentMission === 4 ? 4 : 3;
            if (nextModule > moduleCount) {
                nextMission++;
                nextModule = 1;
            } else if (nextModule < 1) {
                nextMission--;
                nextModule = nextMission === 4 ? 4 : 3;
            }

            // Check bounds
            if (nextMission < 1 || nextMission > 5) return;

            // Navigate
            showModule(nextMission, nextModule);
        }

        function toggleMission(mission) {
            const modules = document.getElementById(`mission-${mission}-modules`);
            const arrow = document.getElementById(`mission-${mission}-arrow`);
            const toggle = document.getElementById(`mission-${mission}-toggle`);

            if (modules.classList.contains('expanded')) {
                modules.classList.remove('expanded');
                arrow.textContent = '‚ñ∂';
                toggle.classList.remove('active');
            } else {
                // Close other missions
                for (let i = 1; i <= 5; i++) {
                    if (i !== mission) {
                        const otherModules = document.getElementById(`mission-${i}-modules`);
                        const otherArrow = document.getElementById(`mission-${i}-arrow`);
                        const otherToggle = document.getElementById(`mission-${i}-toggle`);
                        
                        otherModules.classList.remove('expanded');
                        otherArrow.textContent = '‚ñ∂';
                        otherToggle.classList.remove('active');
                    }
                }

                modules.classList.add('expanded');
                arrow.textContent = '‚ñº';
                toggle.classList.add('active');
            }
        }

        // Initialize
        document.addEventListener('DOMContentLoaded', function() {
            loadTheme();
            updateModuleStatus();
            updateProgressDisplay();
            updateJourneyMap();
        });
    </script>
</body>
</html>